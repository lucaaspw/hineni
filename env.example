# Database
DATABASE_URL="postgresql://your-username:your-password@your-host:5432/your-database"

# JWT Secret (OBRIGATÓRIO - gere uma chave segura aleatória)
# Use: openssl rand -base64 32 ou node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
JWT_SECRET="your-super-secret-jwt-key-here-minimum-32-characters"

# Next Auth (opcional - não usado atualmente)
NEXTAUTH_SECRET="your-nextauth-secret"
NEXTAUTH_URL="http://localhost:3000"

